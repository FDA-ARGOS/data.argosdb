{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/FDA-ARGOS/data.argosdb/v0.7/schema/0.7/uniprot-proteome_*.csv",
    "title": "uniprot-proteome_*.csv",
    "type": "object",
    "required": [
        "uniprotkb_ac",
        "entry_name",
        "status",
        "protein_names",
        "gene_names",
        "organism_name",
        "length"
    ],
    "properties": {
        "uniprotkb_ac": {
            "$id": "#root/uniprotkb_ac ",
            "title": "uniprotkb_ac ",
            "description": "accession for the protein",
            "type": "string",
            "default": "-",
            "examples": [
                "P03466"
            ],
            "pattern": "^.*$"
        },
        "entry_name": {
            "$id": "#root/entry_name ",
            "title": "entry_name ",
            "description": "protein name",
            "type": "string",
            "default": "-",
            "examples": [
                "RDRP_I34A1"
            ],
            "pattern": "^.*$"
        },
        "status": {
            "$id": "#root/status ",
            "title": "status ",
            "description": "Reviewed or unreviewed",
            "type": "string",
            "default": "-",
            "examples": [
                "reviewed"
            ],
            "pattern": "^.*$"
        },
        "protein_names": {
            "$id": "#root/protein_names ",
            "title": "protein_names ",
            "description": "name of protein",
            "type": "string",
            "default": "-",
            "examples": [
                "RNA-directed RNA polymerase catalytic subunit (EC 2.7.7.48) (Polymerase basic protein 1) (PB1) (RNA-directed RNA polymerase subunit P1)"
            ],
            "pattern": "^.*$"
        },
        "gene_names": {
            "$id": "#root/gene_names ",
            "title": "gene_names ",
            "description": "List of gene names associated with this protein",
            "type": "string",
            "default": "-",
            "examples": [
                "PB1"
            ],
            "pattern": "^.*$"
        },
        "organism_name": {
            "$id": "#root/organism ",
            "title": "organism ",
            "description": "Name of the expected organism. Should corrospond to a NCBI Taxonomy node",
            "type": "string",
            "default": "-",
            "examples": [
                "Influenza A virus (strain A/Puerto Rico/8/1934 H1N1)"
            ],
            "pattern": "^.*$"
        },
        "length": {
            "$id": "#root/length ",
            "title": "length ",
            "description": "Total length of the assembled genome.",
            "type": "string",
            "default": "-",
            "examples": [
                "498"
            ],
            "pattern": "^.*$"
        }
    }
}