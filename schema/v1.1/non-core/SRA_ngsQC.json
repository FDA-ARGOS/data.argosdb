{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/FDA-ARGOS/data.argosdb/v1.0/schema/v1.1/non-core/SRA_ngsQC.tsv",
    "title": "SRA_ngsQC.tsv",
    "type": "object",
    "required": [
        "sra_experiment_id",
        "sra_run_id",
        "bioproject",
        "num_of_bases",
        "file_size",
        "published",
        "source",
        "strategy",
        "layout",
        "file_type"
    ],
    "properties": {
        "sra_experiment_id": {
            "$id": "#root/sra_experiment_id",
            "title": "sra_experiment_id",
            "description": "Experiment identifier assigned by SRA and linked to GenBank and other databases.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "SRR9107811"
        },
        "sra_run_id": {
            "$id": "#root/sra_run_id",
            "title": "sra_run_id",
            "description": "The SRA run accession for a set of fastq files ",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "SRX5882434"
        },
        "bioproject": {
            "$id": "#root/bioproject",
            "title": "bioproject",
            "description": "NCBI BioProject DB ID. A BioProject is a collection of biological data for a single initiative, originating from a single organization or from a consortium.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "PRJNA231221"
        },
        "biosample": {
            "$id": "#root/biosample",
            "title": "biosample",
            "description": "NCBI BioSample identifier.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "SAMN11077980"
        },
        "num_of_bases": {
            "$id": "#root/num_of_bases",
            "title": "num_of_bases",
            "description": "Number of nucleotides represented in file",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "400293278"
        },
        "file_size": {
            "$id": "#root/file_size",
            "title": "file_size",
            "description": "File size",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "195921414"
        },
        "published": {
            "$id": "#root/published",
            "title": "published",
            "description": "Date of publication",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "##########"
        },
        "source": {
            "$id": "#root/source",
            "title": "source",
            "description": "Source of genomic information in the file. From an SRA controled vocabulary: GENOMIC: [Genomic DNA (includes PCR products from genomic DNA)], TRANSCRIPTOMIC: [Transcription products or non genomic DNA (EST, cDNA, RT-PCR, screened libraries)], METAGENOMIC : [Mixed material from metagenome], METATRANSCRIPTOMIC: [Transcription products from community targets], SYNTHETIC: [Synthetic DNA], VIRAL RNA: [Viral RNA], OTHER:[Other, unspecified, or unknown library source material (please include additional info in the \u201a\u00c4\u00fadesign description\u201a\u00c4\u00f9)]",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "VIRAL RNA"
        },
        "strategy": {
            "$id": "#root/strategy",
            "title": "strategy",
            "description": "Strategy/method used to generate the NGS. Controled vocabulary consisting of the following terms: WGA WGS WXS RNA-Seq miRNA-Seq WCS CLONE POOLCLONE AMPLICON CLONEEND FINISHING ChIP-Seq MNase-Seq DNase-Hypersensitivity Bisulfite-Seq Tn-Seq EST FL-cDNA CTS MRE-Seq MeDIP-Seq MBD-Seq Synthetic-Long-Read OTHER",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "WGS"
        },
        "layout": {
            "$id": "#root/layout",
            "title": "layout",
            "description": "Sequencing library layout",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "PAIRED"
        },
        "library_name": {
            "$id": "#root/library_name",
            "title": "library_name",
            "description": "Name of sequencing library",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "IL100100209"
        },
        "selection": {
            "$id": "#root/selection",
            "title": "selection",
            "description": "Library selection",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "cDNA"
        },
        "instrument": {
            "$id": "#root/instrument",
            "title": "instrument",
            "description": "Name of instrument used for NGS.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "Illumina MiSeq"
        },
        "file_type": {
            "$id": "#root/file_type",
            "title": "file_type",
            "description": "Type of file representing the genomic data",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "fastq"
        },
        "identified_reads": {
            "$id": "#root/identified_reads",
            "title": "identified_reads",
            "description": "The number of reads that were matched to a reference",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "603880"
        },
        "taxonomy_id": {
            "$id": "#root/taxonomy_id",
            "title": "taxonomy_id",
            "description": "NCBI taxonomy identifier of the organism.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "64320"
        },
        "lineage": {
            "$id": "#root/lineage",
            "title": "lineage",
            "description": "The complete taxonomic lineage for the taxonomic node indicated.",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "Viruses|Riboviria|Orthornavirae|Kitrinoviricota|Flasuviricetes|Amarillovirales|Flaviviridae|Flavivirus|Zika virus"
        },
        "percent_identified": {
            "$id": "#root/percent_identified",
            "title": "percent_identified",
            "description": "The percentage of reads that were matched to a reference",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "0.90817353"
        }
    }
}