            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "-"
        },
        "entry_name": {
            "$id": "#root/entry_name",
            "title": "entry_name",
            "description": "protein name",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "-"
        },
        "status": {
            "$id": "#root/status",
            "title": "status",
            "description": "Reviewed or unreviewed",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "-"
        },
        "protein_names": {
            "$id": "#root/protein_names",
            "title": "protein_names",
            "description": "name of protein",
            "type": "string",
            "default": "N/A",
            "examples": [
                "-"
            ],
            "pattern": "-"
        },
